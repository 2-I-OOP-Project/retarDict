<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.retardict.welcomeSceneController">
    <children>
        <ListView fx:id="list" layoutX="355.0" layoutY="56.0" onMouseClicked="#switchToWordScene"/>
        <TextField fx:id="searchBox" layoutX="350.0" layoutY="30.0" onKeyReleased="#searchWord" prefHeight="25.0"
                   prefWidth="210.0"/>
        <Button layoutX="193.0" layoutY="145.0" mnemonicParsing="false" onAction="#switchToAddWordScene"
                text="Add New Word"/>
        <Button layoutX="203.0" layoutY="215.0" mnemonicParsing="false" onAction="#switchToGameScene" text="Game"/>
    </children>
</AnchorPane>
