<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootAnchor" prefHeight="720.0" prefWidth="1280.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.retardict.addNewWordSceneController">
    <children>
        <TextField fx:id="userDefinedWord" layoutX="555.0" layoutY="38.0" promptText="Word" />
        <TextField fx:id="userDefinedMeaning" layoutX="555.0" layoutY="77.0" promptText="Meaning" />
        <Button layoutX="737.0" layoutY="63.0" mnemonicParsing="false" onAction="#switchBackToMainScene" prefHeight="79.0" prefWidth="208.0" text="Back" />
        <Button fx:id="submitButton" layoutX="585.0" layoutY="112.0" mnemonicParsing="false" onAction="#addUserDefinedWord" text="Submit" />
      <ListView fx:id="list" editable="true" layoutX="426.0" layoutY="165.0" prefHeight="422.0" prefWidth="408.0" />
      <Button fx:id="testButton" layoutX="894.0" layoutY="251.0" mnemonicParsing="false" onAction="#deleteUserDefinedWord" text="test" />
    </children>
</AnchorPane>
