<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootAnchor" prefHeight="720.0" prefWidth="1280.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.retardict.addNewWordSceneController">
    <children>
        <TextField fx:id="userDefinedWord" layoutX="688.0" layoutY="27.0" promptText="Word" />
        <TextField fx:id="userDefinedMeaning" layoutX="688.0" layoutY="66.0" promptText="Meaning" />
        <Button fx:id="submitButton" layoutX="718.0" layoutY="101.0" mnemonicParsing="false" onAction="#addUserDefinedWord" text="Submit" />
      <ListView fx:id="list" editable="true" layoutX="327.0" layoutY="168.0" prefHeight="503.0" prefWidth="871.0" />
      <TextField fx:id="searchBox" layoutX="327.0" layoutY="137.0" onKeyReleased="#searchWord" prefHeight="25.0" prefWidth="871.0" />
    </children>
</AnchorPane>
