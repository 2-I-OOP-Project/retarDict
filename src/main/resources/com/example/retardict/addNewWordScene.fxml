<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="599.0" prefWidth="808.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.retardict.addNewWordSceneController">
    <children>
        <TextField fx:id="userDefinedWord" layoutX="330.0" layoutY="111.0" promptText="Word"/>
        <TextField fx:id="userDefinedMeaning" layoutX="330.0" layoutY="150.0" promptText="Meaning"/>
        <Button layoutX="62.0" layoutY="47.0" mnemonicParsing="false" onAction="#switchBackToMainScene" text="Back"/>
        <Button fx:id="submitButton" layoutX="377.0" layoutY="196.0" mnemonicParsing="false"
                onAction="#addUserDefinedWord" text="Submit"/>
    </children>
</AnchorPane>
