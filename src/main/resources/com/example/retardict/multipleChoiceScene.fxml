<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.retardict.MultipleChoiceSceneController">
   <children>
      <Label fx:id="answerDBox" layoutX="64.0" layoutY="207.0" prefHeight="18.0" prefWidth="300.0" text="Label" />
      <Label fx:id="answerCBox" layoutX="58.0" layoutY="170.0" prefHeight="18.0" prefWidth="312.0" text="Label" />
      <Label fx:id="answerBBox" layoutX="58.0" layoutY="137.0" prefHeight="18.0" prefWidth="312.0" text="Label" />
      <Label fx:id="answerABox" layoutX="58.0" layoutY="104.0" prefHeight="18.0" prefWidth="330.0" text="Label" />
      <Label fx:id="questionBox" layoutX="13.0" layoutY="65.0" prefHeight="18.0" prefWidth="553.0" text="Label" />
      <Button layoutX="14.0" layoutY="100.0" mnemonicParsing="false" onAction="#choseA" text="A" />
      <Button layoutX="14.0" layoutY="133.0" mnemonicParsing="false" onAction="#choseB" text="B" />
      <Button layoutX="14.0" layoutY="167.0" mnemonicParsing="false" onAction="#choseC" text="C" />
      <Button layoutX="14.0" layoutY="204.0" mnemonicParsing="false" onAction="#choseD" text="D" />
      <Button layoutX="254.0" layoutY="276.0" mnemonicParsing="false" onAction="#setQuestion" text="Next Question" />
      <Label fx:id="resultBox" layoutX="286.0" layoutY="246.0" text="Label" />
      <Button layoutX="17.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchBackToGameScene" text="Back" />
   </children>
</AnchorPane>
